#include <stdio.h>
int reverse();
int array[10]={0};
int array_up[10]={0};
int i;
int j;


int main()
{
	setvbuf(stdout, NULL, _IONBF, 0);
	setvbuf(stderr, NULL, _IONBF, 0);
	printf(" Enter the numbers of your array:\n");

	for(i=0;i<10;i++)
	{
		scanf("%d",&array[i]);
	}
	int reverse();
	for(i=0;i<10;i++)
		{
			scanf("%d",&array_up[i]);
		}

}

int reverse()
{
	for(i=0;i<10;i++)
	{
		 array_up[i]=array[10-i];

	}
	return array_up;
}


