#include <stdio.h>
int finder(int m);
int array[10]={0};
int i;
int n;
int value
int main()
{
	setvbuf(stdout, NULL, _IONBF, 0);
	setvbuf(stderr, NULL, _IONBF, 0);
	printf(" Enter the numbers of your array:\n");
	for(i=0;i<10;i++)
	{
		scanf("%d",&array[i]);

	}

	printf("enter the number:\n");
	scanf("%d",&n);
	value=finder[n];
	printf("the value of index %d is %d:\n",n, value);
}

int finder(int m)
{
	for(i=0;i<10;i++)
	{
		if(array[i]=m)
		{
			return array[i];
		}
		}
	}
