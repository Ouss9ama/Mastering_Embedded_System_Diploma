#include <stdio.h>
#include <string.h>
char name[]="OussOuss";
char user[15];
char result;
int main()
{
	setvbuf(stdout, NULL, _IONBF, 0);
	setvbuf(stderr, NULL, _IONBF, 0);
	printf(" Enter the username:\n");
	gets(user);
	result =compare(name,user);
	if(result==1)
	{
		printf("the username is correct");
	}
	else
	{
		printf("the username is wrong");
	}
}

int compare(char x[],char y[])

{
	 if (strcmp(x,y)==0)
	 {
		 return 1;
	 }
	 else
		 return 0;
}

